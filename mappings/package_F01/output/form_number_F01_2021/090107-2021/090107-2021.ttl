  epo:hasTitle "Pre-commercial Procurement (PCP) to Buy R&D (Research and Development) Services for Breakthrough Solutions for 100 % Renewable Energy Supply in Buildings"@en;
  epo:isCoveredByGPA false;
  epo:isJointProcurement true;
