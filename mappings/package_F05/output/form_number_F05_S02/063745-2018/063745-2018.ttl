  epo:hasPurpose epd:id_2018-S-029-063745_LotPurpose_HKNfdBmeACvzBZy3TfjLFj;
  epo:isSubjectToLotSpecificTerm epd:id_2018-S-029-063745_MultipleStageProcedureTerm_HKNfdBmeACvzBZy3TfjLFj;
  epo:isUsingEUFunds false;
